import 'package:flutter/material.dart';

class IzlemeListesi extends StatefulWidget {
  @override
  _IzlemeListesiState createState() => _IzlemeListesiState();
}

class _IzlemeListesiState extends State<IzlemeListesi> {
  List<String> izlemeListesi = [];

  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(
        title: Text('Ä°zleme Listem'),
      ),
      body: ListView.builder(
        itemCount: izlemeListesi.length,
        itemBuilder: (context, index) {
          return ListTile(
            title: Text(izlemeListem[index]),
          );
        },
      ),
    );
  }
}
